<h1>Listing unit_tools</h1>

<table>
  <tr>
    <th>Tool</th>
    <th>Unit</th>
    <th>Path</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @unit_tools.each do |unit_tool| %>
  <tr>
    <td><%= unit_tool.tool_id %></td>
    <td><%= unit_tool.unit_id %></td>
    <td><%= unit_tool.path %></td>
    <td><%= link_to 'Show', unit_tool %></td>
    <td><%= link_to 'Edit', edit_unit_tool_path(unit_tool) %></td>
    <td><%= link_to 'Destroy', unit_tool, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

